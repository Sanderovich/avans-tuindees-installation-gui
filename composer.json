{
    "require": {
        "symfony/http-foundation": "^4.3",
        "symfony/routing": "^4.3",
        "twig/twig": "^2.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
